<h1>All Regions</h1>
<table class="table table-stripped table-hover table-bordered">
  <thead>
  <tr>
    <th>Code</th>
    <th>Name</th>
  </tr>
  </thead>
  <tbody>
  <% @regions.each do |region| %>
  <tr>
    <td><%= region.code %></td>
    <td><%= region.name %></td>
  </tr>
    <% end %>
  </tbody>
</table>